<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.philip.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".SplashActivity"
            android:label="@string/title_activity_splash"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".FunctionListActivity"
            android:label="@string/functions" />
        <activity
            android:name=".WebsiteActivity"
            android:label="@string/title_activity_website" />
        <activity
            android:name=".AboutActivity"
            android:label="@string/title_our_story"
            />
        <activity android:name=".PersonalityActivity"></activity>
    </application>

</manifest>
